{
  "name": "example-react-vite-v6",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm vite\" \"pnpm watch-translations\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "pnpm run prettier --write",
    "format": "pnpm run prettier:fix && pnpm run lint:fix",
    "format:check": "tsc && pnpm run prettier && pnpm run lint",
    "generate-translation-keys": "node scripts/generateTranslationKeys.cjs",
    "watch-translations": "chokidar \"src/i18n/**/*.json\" -c \"pnpm generate-translation-keys\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
