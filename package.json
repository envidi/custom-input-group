    "jotai-devtools": "^0.10.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-color": "^2.19.3",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.0",
    "react-hook-form": "^7.54.2",
